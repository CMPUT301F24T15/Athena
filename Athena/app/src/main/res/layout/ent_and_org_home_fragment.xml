<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/entrant_and_organizer_constraint_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context=".EntrantAndOrganizerFragments.homeScreen">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/entrant_and_organizer_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#D7F2EE"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <androidx.fragment.app.FragmentContainerView
            android:id="@+id/content_frame"
            android:layout_width="412dp"
            android:layout_height="0dp"
            android:layout_marginTop="34dp"
            app:layout_constraintBottom_toTopOf="@+id/imageView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintTop_toBottomOf="@+id/app_name"
            app:layout_constraintVertical_bias="1.0">

        </androidx.fragment.app.FragmentContainerView>

        <TextView
            android:id="@+id/app_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/bai_jamjuree_bold"
            android:text="@string/app_name"
            android:textAllCaps="true"
            android:textColor="@color/title_text_color"
            android:textSize="34sp"
            android:typeface="normal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.481"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.004" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="410dp"
            android:layout_height="78dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.333"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.998"
            app:srcCompat="@drawable/bottombarcoloring" />


        <ImageButton
            android:id="@+id/profile_picture_button"
            android:layout_width="51dp"
            android:layout_height="46dp"
            android:backgroundTint="#D7F2EE"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.994"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.043"
            app:srcCompat="@drawable/profilepicturebutton" />

        <ImageButton
            android:id="@+id/more_options_button"
            android:layout_width="54dp"
            android:layout_height="28dp"
            android:backgroundTint="#D7F2EE"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.045"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.061"
            app:srcCompat="@drawable/moreoptionsbutton" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/drawer_layer"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0">
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_editor_absoluteX="0dp">

            <ImageView
                android:id="@+id/text_addon_non_edit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="872dp"
                android:layout_marginBottom="3dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.082"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/more_options_drawer"
                app:srcCompat="@drawable/events_attending_text_addon" />

            <ImageButton
                android:id="@+id/check_events_button"
                android:layout_width="120dp"
                android:layout_height="71dp"
                android:backgroundTint="#93BDB6"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.054"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.989"
                app:srcCompat="@drawable/checkeventsbutton" />

            <ImageButton
                android:id="@+id/check_updates_button"
                android:layout_width="125dp"
                android:layout_height="79dp"
                android:layout_marginTop="812dp"
                android:backgroundTint="#93BDB6"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.993"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="1.0"
                app:srcCompat="@drawable/updatesbuttonimage" />

            <ImageButton
                android:id="@+id/scan_qr_code_button"
                android:layout_width="120dp"
                android:layout_height="80dp"
                android:backgroundTint="#93BDB6"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.498"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="1.0"
                app:srcCompat="@drawable/scanqrcodebutton" />

            <LinearLayout
                android:id="@+id/more_options_drawer"
                android:layout_width="141dp"
                android:layout_height="0dp"
                android:background="#90C5BD"
                android:orientation="vertical"
                android:visibility="gone"
                app:layout_constraintBottom_toTopOf="@+id/scan_qr_code_button"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageButton
                        android:id="@+id/create_event_button"
                        android:layout_width="141dp"
                        android:layout_height="64dp"
                        android:layout_marginTop="92dp"
                        android:backgroundTint="@color/drawer_background_color"
                        android:src="@drawable/create_event_drawer_button"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <ImageButton
                        android:id="@+id/events_im_hosting_button"
                        android:layout_width="0dp"
                        android:layout_height="74dp"
                        android:layout_marginTop="2dp"
                        android:backgroundTint="@color/drawer_background_color"
                        android:src="@drawable/events_hosting_button"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="1.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/create_event_button" />

                    <ImageButton
                        android:id="@+id/close_drawer_button"
                        android:layout_width="45dp"
                        android:layout_height="37dp"
                        android:layout_marginStart="99dp"
                        android:layout_marginTop="2dp"
                        android:layout_marginBottom="10dp"
                        android:backgroundTint="@color/drawer_background_color"
                        android:src="@drawable/left_arrow_drawer"
                        app:layout_constraintBottom_toTopOf="@+id/create_event_button"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.0" />

                    <TextView
                        android:id="@+id/textView5"
                        android:layout_width="100dp"
                        android:layout_height="35dp"
                        android:text="close drawer"
                        android:textAlignment="viewEnd"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toTopOf="@+id/create_event_button"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="1.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/close_drawer_button"
                        app:layout_constraintVertical_bias="0.0" />

                    <ImageButton
                        android:id="@+id/adminProfileBrowse"
                        android:layout_width="139dp"
                        android:layout_height="72dp"
                        android:layout_marginTop="8dp"
                        android:backgroundTint="@color/drawer_background_color"
                        android:src="@drawable/admin_profile_browse_button"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/events_im_hosting_button"
                        app:layout_constraintVertical_bias="0.0" />

                    <ImageButton
                        android:id="@+id/adminImageBrowse"
                        android:layout_width="139dp"
                        android:layout_height="76dp"
                        android:layout_marginTop="4dp"
                        android:layout_marginEnd="2dp"
                        android:layout_marginBottom="324dp"
                        android:backgroundTint="@color/drawer_background_color"
                        android:src="@drawable/browse_images"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/adminEventsBrowse"
                        app:layout_constraintVertical_bias="0.0" />

                    <ImageButton
                        android:id="@+id/adminEventsBrowse"
                        android:layout_width="143dp"
                        android:layout_height="65dp"
                        android:layout_marginTop="8dp"
                        android:backgroundTint="@color/drawer_background_color"
                        android:src="@drawable/browse_events_button"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/adminProfileBrowse"
                        app:layout_constraintVertical_bias="0.0" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </LinearLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>